    "name": "arkOS Assistant",
    "platform": "all",
    "arch": "all",
    "version": "0.37.6",
    "app-bundle-id": "io.citizenweb.arkOS-Assistant",
    "app-category-type": "public.app-category.utilities",
    "app-copyright": "(c) 2016 CitizenWeb",
    "app-version": "1.0.0",
